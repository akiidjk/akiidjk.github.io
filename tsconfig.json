{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@/utils": ["./src/utils/*"],
      "@/components": ["./src/components/*"],
      "@/layouts": ["./src/layouts/*"],
      "@/pages": ["./src/pages/*"],
      "@/styles": ["./src/styles/*"],
      "@/assets": ["./src/assets/*"],
      "@/constants": ["./src/constants/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
